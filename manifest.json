{
  "name": "CopyNote",
  "version": "1.0",
  "manifest_version": 2,
  "description": "CopyNote",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
   
    "contextMenus",
    "<all_urls>"
  ],
  "background": {
    "persistent": false,
    "scripts": ["contextMenu.js"]
  }
}

